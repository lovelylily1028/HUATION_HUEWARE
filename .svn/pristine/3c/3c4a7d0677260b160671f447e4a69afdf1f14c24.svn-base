/* common */
/* 달력 아이콘 */
.ico_calendar {position:relative;}
.ico_calendar img {position:absolute;right:3px;top:4px;cursor:pointer;}
/* 필수입력사항텍스트(페이지) */
.must_txt {padding-top:14px;font-size:11px;color:#ff6600;}
.must_txt img {padding-top:5px;padding-right:5px;vertical-align:top;}
/* 필수입력사항텍스트(팝업) */
#contentWp .conTop_area .must_txt {margin:-2px 0 -2px 0;padding-top:0;}
/* 필수입력사항텍스트(레이어팝업) */
#contentLp .conTop_area .must_txt {margin:-2px 0 -2px 0;padding-top:0;}



/* 테이블Out  가이드 텍스트(페이지) */
.guide_txt {padding:13px 0 0 16px;background:url(../images/common/ico_guide.gif) no-repeat 0 14px;font-size:11px;color:#ff6600;}/* 테이블 Bottom */
.Tguide_txt {padding:14px 0 0 16px;background:url(../images/common/ico_guide.gif) no-repeat 0 15px;font-size:11px;color:#ff6600;}/* 테이블 Top */
/* 테이블Out  가이드 텍스트(팝업) */
#contentWp .conTop_area .Tguide_txt {margin:-2px 0 -2px 0;padding:0 0 0 16px;background-position:0 1px;}/* 테이블 Top */

/* search */
.search {float:left;}
.search li {float:left;padding-left:5px;}
.search li select {float:left;height:27px;line-height:21px;padding:4px;font-size:11px;}/* ine-height:21px; 사파리 높이 사이즈 설정 */
.search li input.text {width:220px;height:19px;line-height:15px;padding:3px 5px;font-size:11px;}
.search li input.textdate {width:90px;}
.search li .ico_calendar img {right:5px;top:5px;}
.search li .btn_type01 {margin:0 5px 0 -6px;}

/* table */
/* tbl_type 기본 */
.tbl_type {table-layout:fixed;word-break:break-all;/*word-wrap:break-word;*/}/* 테이블 고정 */
.tbl_type {width:100%;border-top:2px solid #3ba90d;border-left:1px solid #ddd;}
.tbl_type th, .tbl_type td {padding:5px 15px 7px;height:23px;border-bottom:1px solid #ddd;border-right:1px solid #ddd;text-align:left;vertical-align:middle;}
.tbl_type th {padding:5px 15px 7px;background:#f0f0f0;color:#333;}
.tbl_type th .must_ico {position:relative;}/* 필수입력아이콘 */
.tbl_type th .must_ico img {position:absolute;top:-5px;left:203px;}
.tbl_type td .listD li {padding-top:6px;}/* 테이블내 줄바꿈 형태 리스트 */
.tbl_type td .listD li.first {padding-top:0;}
.tbl_type td.listT {padding:0 !important;}/* 테이블내 테이블(타이틀왼쪽) */
.tbl_type td.listT .tbl_type {border:0 none !important;}
.tbl_type td.listT .tbl_type th {background:#f5f5f5;color:#666;}
.tbl_type td.listT .tbl_type tr.last th, .tbl_type td.listT .tbl_type tr.last td {border-bottom:0 none;}/* 테이블내 등록테이블 추가로 마지막 라인 2줄 표시. 1줄 감추는 설정 */
.tbl_type td.listT .tbl_type tr td.last {border-right:0 none;}
.tbl_type td.listT01 {padding:0 !important;}/* 테이블내 테이블(타이틀위) */
.tbl_type td.listT01 dt {padding:5px 15px 7px;line-height:23px;}
.tbl_type td.listT01 dd .tbl_type {border:0 none;;border-top:1px solid #ddd;}
.tbl_type td.listT01 dd .tbl_type th {background:#f5f5f5;color:#666;text-align:center;}
.tbl_type td.listT01 dd .tbl_type tr th.last, .tbl_type td.listT01 dd .tbl_type tr td.last {border-right:0 none;}
.tbl_type td.listT01 dd .tbl_type tr td.last a.btn_type03 {margin-left:0;}/* 테이블내 테이블(타이틀위)-리스트 테이블 안 버튼 설정 */
.tbl_type td.listT01 dd .tbl_type {margin-bottom:-1px;}/* 테이블내 리스트테이블 추가로 마지막 라인 2줄 표시. 1줄 감추는 설정 */
.tbl_type td input, .tbl_type td select {margin-top:1px;margin-bottom:-1px;}/* input 위치 설정 */
.tbl_type input.check {margin-top:0;}/* 체크박스 위치 설정 */
.tbl_type input.radio {margin-top:-2px;}/* 라디오 위치 설정 */
input.check.md0, input.radio.md0 {margin-left:0 !important;}/* 처음나오는 체크박스, 라디오 margin 초기화 */
.tbl_type .guide_txt {padding:0;padding-top:1px;background:none;color:#00a651;vertical-align:middle;}/* 테이블내 가이드 텍스트 */
.tbl_type .guide_txt.br {padding-top:5px;}/* 기존 내용에서 줄바꿈 한뒤 가이드 텍스트가 오는 경우(옆으로 오는 경우 아님) */
.tbl_type .guide_txt strong {color:#ff6600;}
/* tbl_type_list */
.tbl_type_list th, .tbl_type_list td {padding:5px 5px 7px;text-align:center;}
.tbl_type_list input.check.md0, .tbl_type_list input.radio.md0 {margin-left:0 !important;margin-right:0 !important;}/* 리스트테이블 체크박스, 라디오 솔로일때 margin 초기화 */
.tbl_type_list td a {text-decoration:underline;}
.tbl_type_list td a span {text-decoration:underline;}
.oddColor {background:#fff;}/* 홀수 */
.evenColor {background:#fafafa;}/* 짝수  */
.tbl_type_list a.btn_type03 {margin-left:0;}/* 리스트 테이블 안 버튼 설정(버튼1개) */
/* tbl_type_scrollY(테이블안 tbody안 스크롤 생성일 경우 table 설정) */
.tbl_type_scrollY {padding:0 !important;border-bottom:0 none !important;background:#fff;}
.tbl_type_scrollY .scrollY {overflow-y:scroll;}
.tbl_type_scrollY .scrollY .tbl_type {border-top:0 none;border-left:0 none;}
/* tbl_type 정보테이블 */
.tbl_type_info th, .tbl_type_info td {line-height:15px;text-align:center;}

/* btn */
/* table out small */
a.btn_type01 {margin:0 5px;padding-left:14px;background:url(../images/common/btn_bg.gif) no-repeat;vertical-align:middle;color:#fff;font-weight:bold;}
a.btn_type01 span {padding:5px 14px 7px 0;background:url(../images/common/btn_bg.gif) no-repeat right 0;}
a.btn_type01_gray {background-position:0 -27px;color:#666;}/* table out small gray */
a.btn_type01_gray span {background-position:right -27px;}
/* table out big */
a.btn_type02 {margin:0 5px;padding-left:24px;background:url(../images/common/btn_bg.gif) no-repeat 0 -54px;vertical-align:middle;color:#fff !important;font-weight:bold;}
a.btn_type02 span {padding:7px 24px 9px 0;background:url(../images/common/btn_bg.gif) no-repeat right -54px;}
a.btn_type02_gray {background-position:0 -85px;color:#666 !important;}/* table out big gray */
a.btn_type02_gray span {background-position:right -85px;}
a.btn_type02_orange {background-position:0 -214px;}/* table out big orange */
a.btn_type02_orange span {background-position:right -214px;}
/* table out 공통 */
a.md0 {margin-right:0 !important;}/* 오른쪽 정렬일때 오른쪽 마지막 버튼 margin 초기화 */
.Tbtn_areaR {float:left;}/* Top 버튼영역 오른쪽 정렬 */
.Bbtn_areaC {position:relative;padding-top:15px;text-align:center;}/* Bottom 버튼영역 가운데 정렬 */
.Bbtn_areaR {padding-top:15px;text-align:right;}/* Bottom 버튼영역 오른쪽 정렬 */
.Bbtn_areaC .Bbtn_areaRFix {position:absolute;top:15px;right:-5px;}/* Bottom 버튼영역 가운데, 오른쪽 정렬 */
/* table in */
a.btn_type03 {margin:1px 0 -1px 5px;padding-left:9px;height:23px;line-height:23px;background:url(../images/common/btn_bg.gif) no-repeat 0 -116px;vertical-align:middle;color:#666;font-size:11px;}
a.btn_type03 span {padding-right:9px;background:url(../images/common/btn_bg.gif) no-repeat right -116px;text-decoration:none !important;}

/* paging */
.paging {padding:12px 0;text-align:center;border:1px solid #ddd;border-top:0 none;}
.paging a, .paging strong {padding:2px 4px 3px;}
.paging a.direction {padding:0px;}
.paging strong {color:#ff6600;}
.paging .btn {display:inline-block;padding:0;width:21px;height:21px;background:url(../images/common/btn_bg.gif) no-repeat;}
.paging .btn_first {background-position:0 -139px;}
.paging .btn_prev {margin-right:10px;width:22px;background-position:-21px -139px;}
.paging .btn_next {margin-left:10px;width:22px;background-position:-43px -139px;}
.paging .btn_last {background-position:-65px -139px;}

/* ellipsis */
.ellipsis {display:block;width:100%;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;line-height:18px;}
.ellipsis a, .ellipsis strong {display:inline;}

/* error */
.error {position:absolute;top:50%;left:50%;margin-top:-75px; margin-left:-180px; width:360px;height:150px;color:#333;font-size:14px;line-height:150%;}
.error strong {padding-top:10px;}
.error strong a {color:#00a651;text-decoration:underline;}
/* //common */


/* main_contents */
