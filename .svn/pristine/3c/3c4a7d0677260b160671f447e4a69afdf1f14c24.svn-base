.main_board div ul li a .ellipsis {display:inline-block;width:400px;line-height:35px;}/* 타이틀 말줄임 */
.main_board div ul li.first a {background:url(../images/main/list_first_bg.gif) no-repeat;}
.main_board div ul li a:hover {background:url(../images/main/list_over_bg.gif) no-repeat;color:#333;font-weight:bold;}
