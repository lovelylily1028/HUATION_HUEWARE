<%@ page contentType="text/html; charset=euc-kr"%><%@ page import = "com.huation.common.waf.*"%><html><head><title>:: Huation-관리 시스템 ::</title><meta http-equiv="Content-Type" content="text/html; charset=euc-kr"><script language = "javascript">	try{		var version = navigator.appVersion;		var versionText="";		strTitle=version.split(';');		for(var i=0 ; i< strTitle.length ; i++){			if(i==1){				versionText = trim(strTitle[i]);			}		}		if(versionText=="MSIE7.0"){			window.opener.open('about:blank','_self').close();		}else{			window.opener.opener = window;			window.opener.close();		}	}catch(e){}	function trim(targetStr){		var eliminateStr = /\s+/g;		return targetStr.replace(eliminateStr,"");	}</script></head><frameset rows="53,*" cols="*" frameborder="NO" border="0" framespacing="0">	<frame src="<%= request.getContextPath()%>/jsp/hueware/common/top.jsp" name="topFrame" scrolling="NO" noresize>	<frameset id="downFrame" cols="224,*" frameborder="NO" border="0" framespacing="0">		<frame src="<%= request.getContextPath()%>/jsp/hueware/common/left.jsp" name="leftFrame" scrolling="NO" noresize>		<frame src="" name="mainFrame" scrolling="auto">	</frameset></frameset><noframes><body></body></noframes></html>